    "test": "mocha --reporter spec test/*-test.js",
    "test-browser": "BCOIN_NO_NATIVE=1 BCOIN_USE_ELLIPTIC=1 mocha --reporter spec test/*-test.js",
    "test-file": "mocha --reporter spec",
    "webpack": "webpack"
