  "repository": "git://github.com/bcoin-org/bcoin.git",
  "homepage": "https://github.com/bcoin-org/bcoin",
  "bugs": {
    "url": "https://github.com/bcoin-org/bcoin/issues"
  },
  "author": "Fedor Indutny <fedor@indutny.com>",
