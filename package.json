    "socket.io": "2.0.1",
    "socket.io-client": "2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.1.0",
    "hash.js": "^1.0.3",
    "jsdoc": "^3.4.3",
    "level-js": "^2.2.4",
    "mocha": "^3.4.1",
